{
    "name": "phpcheckstyle/phpcheckstyle",
    "description": "PHPCheckstyle is an open-source tool that helps PHP programmers adhere to certain coding conventions",
    "type": "library",
    "keywords": ["checkstyle","standard","convention", "coding standards", "validator"],
    "homepage": "https://github.com/phpcheckstyle/phpcheckstyle/",
    "license": "OSL-2.1",
    "authors": [
        {
            "name": "James Brooks",
            "email": "jbrooksuk@me.com",
            "homepage": "http://james-brooks.uk",
            "role": "Developer"
        },
        {
            "name": "Benoit Pesty",
            "homepage": "https://github.com/tchule",
            "role": "Developer"
        },
        {
            "name": "Marcin Kurczewski",
            "homepage": "http://sakuya.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-iconv": "*",
        "ext-tokenizer": "*",
        "nikic/php-parser": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "support" : {
        "wiki" : "https://github.com/phpcheckstyle/phpcheckstyle/wiki",
        "source" : "https://github.com/phpcheckstyle/phpcheckstyle"
    },
    "autoload": {
        "psr-4": {
            "PHPCheckstyle\\PHPCheckstyle\\": "src",
            "PHPCheckstyle\\PHPCheckstyle\\Tests\\": "tests"
        }
    }

}
